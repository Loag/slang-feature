{
  "id": "slang",
  "version": "0.0.2",
  "name": "Slang",
  "documentationURL": "https://github.com/loag/slang-feature",
  "description": "Installs slangc compiler",
  "options": {
    "version": {
      "type": "string",
      "enum": [
        "2024.17",
        "2024.16",
        "2024.15",
        "2024.14"
      ],
      "default": "2024.17",
      "description": "version of slang to install"
    }
  },
  "mounts": [],
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils"
  ],
  "containerEnv": {
    "PATH": "/opt/slang/bin:${PATH}"
  }
}
